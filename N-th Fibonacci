# https://www.codewars.com/kata/n-th-fibonacci/train/python

def nth_fib(n):
    first = 0
    second = 1
    third = 0
    temp = 0
    fibo_digits = [0,1]
    while temp <= n:
        third = first + second
        fibo_digits.append(third)
        first = second
        second = third
        temp += 1
    return fibo_digits[n-1]
